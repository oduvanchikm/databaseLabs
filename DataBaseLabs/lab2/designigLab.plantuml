@startuml
!define RECTANGLE class

RECTANGLE Products {
  + ProductID
  + Model
  + Color
  + Size
  + Price
  + Stock
}

RECTANGLE Client {
  + ClientID
  + ClientName
  + Phone
  + Email
  + Address
}

RECTANGLE Order {
  + OrderID
  + ClientID
  + OrderDate
  + TotalAmount
}

RECTANGLE OrderProducts {
  + OrderProductsID
  + OrderID
  + ProductID
  + Quantity
  + Price
}

RECTANGLE Material {
  + MaterialID
  + MaterialName
  + Stock
}

RECTANGLE ProductsMaterial {
  + ProductsMaterialID
  + MaterialID
  + ProductID
  + Quantity
}

RECTANGLE Supplier {
  + SupplierID
  + SupplierName
  + Phone
  + Email
  + Address
}

RECTANGLE MaterialSupplier {
  + MaterialSupplierID
  + SupplierID
  + MaterialID
  + Quantity
  + SupplyDate
}

Client " " --> " " Order
Order " " --> " " OrderProducts
Products " " --> " " OrderProducts
Material " " --> " " ProductsMaterial
Products " " --> " " ProductsMaterial
Supplier " " --> " " MaterialSupplier
Material " " --> " " MaterialSupplier

@Material " " --> " " MaterialSupplier

@enduml
